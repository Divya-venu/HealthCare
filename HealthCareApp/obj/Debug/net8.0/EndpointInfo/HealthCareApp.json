  "components": {
    "schemas": {
      "Asset": {
        "required": [
          "language",
          "name",
          "type"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "type": {
            "$ref": "#/components/schemas/AssetType"
          },
          "language": {
            "$ref": "#/components/schemas/AssetLanguage"
          }
        },
        "additionalProperties": false
      },
      "AssetLanguage": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "AssetType": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "ContentFile": {
        "type": "object",
        "properties": {
          "filename": {
            "type": "string",
