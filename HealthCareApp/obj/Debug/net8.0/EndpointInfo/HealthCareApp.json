          "engagement": {
            "type": "number",
            "format": "double"
          },
          "asset": {
            "$ref": "#/components/schemas/Asset"
          },
          "dwellTime": {
            "type": "string",
            "nullable": true
          },
          "effectiveness": {
            "$ref": "#/components/schemas/Effectiveness"
          },
          "campaignId": {
            "type": "string",
            "nullable": true
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "PatientLanguage": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "RegisterDto": {
        "required": [
          "email",
          "password",
          "role"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 6,
            "type": "string"
          },
          "role": {
            "minLength": 1,
            "type": "string"
          },
          "firstName": {
            "type": "string",
            "nullable": true
          },
          "lastName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "Please enter into field the word 'Bearer' followed by a space and the JWT value.",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
