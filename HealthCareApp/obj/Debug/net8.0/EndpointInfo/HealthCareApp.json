            "minLength": 1,
            "type": "string"
          },
          "therapy": {
            "minLength": 1,
            "type": "string"
          },
          "city": {
            "minLength": 1,
            "type": "string"
          },
          "reach": {
            "type": "integer",
            "format": "int32"
          },
          "shares": {
            "type": "integer",
            "format": "int32"
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "startDate": {
            "type": "string",
            "format": "date-time"
          },
          "endDate": {
            "type": "string",
            "format": "date-time"
          },
          "contentPack": {
            "minLength": 1,
            "type": "string"
          },
          "targetAudience": {
            "type": "string",
            "nullable": true
          },
          "language": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateMedicalContentDto": {
        "required": [
          "description",
          "expiryDate",
          "language",
          "medicalName"
        ],
        "type": "object",
        "properties": {
          "medicalName": {
            "minLength": 1,
            "type": "string"
          },
          "description": {
            "minLength": 1,
            "type": "string"
          },
          "language": {
            "minLength": 1,
            "type": "string"
          },
          "contentFiles": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ContentFile"
            },
            "nullable": true
          },
          "expiryDate": {
            "type": "string",
            "format": "date-time"
          },
          "reminderFrequency": {
            "type": "string",
            "nullable": true
          },
          "onLabel": {
            "type": "boolean"
          },
          "notes": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Effectiveness": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "Hcp": {
        "required": [
          "city",
          "email",
          "name",
          "specialty"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "specialty": {
            "minLength": 1,
            "type": "string"
          },
          "city": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string"
          },
          "phone": {
            "type": "string",
            "nullable": true
          },
          "shares": {
            "type": "integer",
            "format": "int32"
          },
          "engagementScore": {
            "type": "number",
            "format": "double"
          },
          "lastActive": {
            "type": "string",
            "format": "date-time"
          },
          "isActive": {
            "type": "boolean"
          },
          "campaigns": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "totalEngagement": {
            "$ref": "#/components/schemas/HcpTotalEngagement"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "HcpTotalEngagement": {
        "type": "object",
        "properties": {
          "views": {
            "type": "integer",
            "format": "int32"
          },
          "shares": {
            "type": "integer",
            "format": "int32"
          },
          "completions": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "HcpUpdateEngagementDto": {
        "required": [
          "totalEngagement"
        ],
        "type": "object",
        "properties": {
          "totalEngagement": {
            "$ref": "#/components/schemas/HcpTotalEngagement"
          }
        },
        "additionalProperties": false
      },
      "LoginDto": {
        "required": [
          "email",
          "password",
          "role"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
