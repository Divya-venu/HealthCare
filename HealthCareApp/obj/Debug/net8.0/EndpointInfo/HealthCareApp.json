          },
          "password": {
            "minLength": 1,
            "type": "string"
          },
          "role": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "PatientEngagement": {
        "required": [
          "asset",
          "city",
          "completions",
          "language",
          "week"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "nullable": true
          },
          "week": {
            "minLength": 1,
            "type": "string"
          },
          "completions": {
            "type": "integer",
            "format": "int32"
          },
          "change": {
            "type": "integer",
            "format": "int32"
          },
          "city": {
            "minLength": 1,
            "type": "string"
          },
          "language": {
            "$ref": "#/components/schemas/PatientLanguage"
